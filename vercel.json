{
    "buildCommand": "yarn build",
    "outputDirectory": "client/dist",
    "installCommand": "yarn install",
    "framework": null,
    "ignoreCommand": "git diff HEAD^ HEAD --quiet .",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
